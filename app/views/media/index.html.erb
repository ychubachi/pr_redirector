<h1>Listing media</h1>

<table>
  <tr>
    <th>Category</th>
    <th>Title</th>
    <th>Start date</th>
    <th>End date</th>
    <th>Cost</th>
    <th>Copies</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @media.each do |medium| %>
  <tr>
    <td><%= medium.category %></td>
    <td><%= medium.title %></td>
    <td><%= medium.start_date %></td>
    <td><%= medium.end_date %></td>
    <td><%= medium.cost %></td>
    <td><%= medium.copies %></td>
    <td><%= link_to 'Show', medium %></td>
    <td><%= link_to 'Edit', edit_medium_path(medium) %></td>
    <td><%= link_to 'Destroy', medium, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

